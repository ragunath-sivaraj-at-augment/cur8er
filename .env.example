# Environment variables for Cur8er
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (required for DALL-E)
OPENAI_API_KEY=your_openai_api_key_here

# Google API Key (required for Imagen, works for Nano Banana too)
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Dedicated Nano Banana API Key (if you have one)
# NANO_BANANA_API_KEY=your_nano_banana_api_key_here

# Hugging Face API Token (optional, for Stable Diffusion)
HUGGINGFACE_API_KEY=your_huggingface_token_here

# Application settings
DEBUG=False
CACHE_IMAGES=True
MAX_FILE_SIZE=10485760